<?xml version="1.0" encoding="UTF-8"?> 
<Context reloadable="true">
	<Resource name="jdbc/MubiDB"
		auth="Container"
		type="javax.sql.DataSource"
		maxActive="100"
		maxIdle="30"
		maxWait="10000"
		username="SA"
		password=""
		driverClassName="org.hsqldb.jdbcDriver"
		url="jdbc:hsqldb:mem:.?autoReconnect=true"/>
</Context>